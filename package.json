{
  "name": "cdk-monitoring",
  "version": "1.0.0",
  "private": true,
  "scripts": {
    "build": "tsc",
    "watch": "tsc -w",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "// === CODE HEALTH ===": "",
    "find:unused": "knip",
    "find:unused:fix": "knip --fix",
    "find:unused:watch": "knip --watch",
    "// === DEPENDENCY ANALYSIS ===": "",
    "deps:check": "depcruise bin lib lambda --config .dependency-cruiser.cjs",
    "deps:check:ci": "depcruise bin lib lambda --config .dependency-cruiser.cjs --output-type err-long",
    "deps:orphans": "depcruise bin lib lambda --config .dependency-cruiser.cjs --output-type err --focus orphan",
    "// === DEPENDENCY GRAPHS ===": "",
    "deps:graph": "depcruise bin lib --include-only '^(bin|lib)/' --output-type dot --config .dependency-cruiser.cjs | dot -T svg > docs/dependency-graph.svg",
    "deps:graph:stacks": "depcruise lib/stacks --include-only '^lib/' --output-type dot --config .dependency-cruiser.cjs | dot -T svg > docs/stacks-graph.svg",
    "deps:graph:common": "depcruise lib/common --include-only '^lib/' --output-type dot --config .dependency-cruiser.cjs | dot -T svg > docs/common-graph.svg",
    "deps:graph:lambda": "depcruise lambda --output-type dot --config .dependency-cruiser.cjs | dot -T svg > docs/lambda-graph.svg",
    "deps:graph:html": "depcruise bin lib lambda --include-only '^(bin|lib|lambda)/' --output-type html --config .dependency-cruiser.cjs > docs/dependency-report.html",
    "// === ARCHITECTURE VALIDATION ===": "",
    "deps:archi": "depcruise bin lib --include-only '^(bin|lib)/' --output-type archi --config .dependency-cruiser.cjs | dot -T svg > docs/architecture.svg",
    "deps:graphs": "yarn deps:graph && yarn deps:graph:stacks && yarn deps:graph:common && yarn deps:graph:lambda && yarn deps:graph:html && yarn deps:archi && echo 'âœ“ All dependency graphs generated in docs/'",
    "// === COMBINED HEALTH CHECK ===": "",
    "health": "npm run lint && npm run find:unused && npm run deps:check",
    "health:ci": "npm run lint && npm run find:unused && npm run deps:check:ci",
    "// === CLEANUP HELPERS ===": "",
    "clean:backups": "find . -name '*.backup' -o -name '*.bak' -o -name '*.backup.ts' | grep -v node_modules",
    "clean:backups:delete": "find . \\( -name '*.backup' -o -name '*.bak' -o -name '*.backup.ts' \\) -not -path './node_modules/*' -delete",
    "// === UNIT TESTS ===": "",
    "test:unit": "jest tests/unit --coverage",
    "test:vpc": "jest tests/unit/stacks/vpc",
    "test:verbose": "VERBOSE_TESTS=1 jest --verbose",
    "test:validate-patterns": "bash scripts/validate-test-patterns.sh",
    "// === LINTING ===": "",
    "lint": "eslint . --max-warnings 50",
    "lint:fix": "eslint . --fix",
    "lint:stacks": "yarn lint:cdk",
    "lint:cdk": "tsc && ENABLE_CDK_NAG=false cdk ls > /dev/null && echo 'CDK validation successful'",
    "typecheck": "tsc --noEmit",
    "cdk": "cdk",
    "deploy": "cdk deploy",
    "synth": "cdk synth",
    "integ-runner": "integ-runner",
    "integ:networking": "integ-runner --test-regex networking",
    "integ:update": "integ-runner --update-on-failed",
    "knip": "knip",
    "// === DOCUMENTATION ===": "",
    "docs": "typedoc",
    "docs:watch": "typedoc --watch",
    "docs:serve": "npx serve docs/api",
    "docs:monitoring": "typedoc --entryPoints ./lib/projects/monitoring ./lib/stacks/monitoring --out docs/monitoring --name 'Monitoring Stack'",
    "docs:compute": "typedoc --entryPoints ./lib/common/compute --out docs/compute --name 'Compute Constructs'",
    "docs:nextjs": "typedoc --entryPoints ./lib/projects/nextjs ./lib/stacks/nextjs --out docs/nextjs --name 'Next.js Stack'",
    "// === CDK CLI (Interactive) ===": "",
    "cli": "tsx scripts/deployment/cli.ts",
    "cli:synth": "tsx scripts/deployment/cli.ts synth",
    "cli:deploy": "tsx scripts/deployment/cli.ts deploy",
    "cli:diff": "tsx scripts/deployment/cli.ts diff",
    "cli:destroy": "tsx scripts/deployment/cli.ts destroy",
    "cli:list": "tsx scripts/deployment/cli.ts list",
    "cli:sync-dashboards": "tsx scripts/deployment/cli.ts sync-dashboards"
  },
  "lint-staged": {
    "**/*.test.ts": [
      "eslint --fix",
      "bash scripts/validate-test-patterns.sh"
    ],
    "**/*.ts": [
      "eslint --fix"
    ]
  },
  "devDependencies": {
    "@aws-cdk/integ-runner": "latest",
    "@aws-cdk/integ-tests-alpha": "latest",
    "@aws-sdk/client-acm": "^3.978.0",
    "@aws-sdk/client-route-53": "^3.978.0",
    "@aws-sdk/client-sts": "^3.978.0",
    "@babel/core": "^7.28.5",
    "@babel/preset-env": "^7.28.5",
    "@babel/preset-typescript": "^7.28.5",
    "@jest/test-sequencer": "^30.2.0",
    "@types/aws-lambda": "^8.10.159",
    "@types/inquirer": "^9.0.9",
    "@types/jest": "^29.5.11",
    "@types/node": "^25.2.0",
    "@types/source-map-support": "^0.5.10",
    "@typescript-eslint/eslint-plugin": "^8.18.1",
    "@typescript-eslint/parser": "^8.18.1",
    "aws-cdk": "^2.232.1",
    "aws-sdk-client-mock": "^4.1.0",
    "aws-sdk-client-mock-jest": "^4.1.0",
    "babel-jest": "^30.2.0",
    "dependency-cruiser": "^17.3.7",
    "esbuild": "^0.27.3",
    "eslint": "^9.32.0",
    "eslint-import-resolver-typescript": "^3.7.0",
    "eslint-plugin-import": "^2.28.0",
    "eslint-plugin-jest": "^29.12.1",
    "eslint-plugin-jest-extended": "^3.0.1",
    "eslint-plugin-jsdoc": "^46.8.0",
    "husky": "^9.1.7",
    "inquirer": "^13.2.2",
    "jest": "^30.2.0",
    "jest-junit": "^16.0.0",
    "knip": "^5.82.1",
    "lint-staged": "^16.2.7",
    "prettier": "3.8.0",
    "ts-jest": "^29.1.1",
    "ts-node": "^10.9.2",
    "ts-prune": "^0.10.3",
    "ts-prune-ex": "^0.0.5",
    "tsx": "^4.21.0",
    "turbo": "^2.6.1",
    "typedoc": "^0.28.17",
    "typedoc-plugin-markdown": "^4.9.0",
    "typescript": "^5.9.3",
    "typescript-eslint": "^8.18.1"
  },
  "dependencies": {
    "@aws-lambda-powertools/logger": "^2.30.2",
    "@aws-sdk/client-api-gateway": "^3.943.0",
    "@aws-sdk/client-auto-scaling": "^3.968.0",
    "@aws-sdk/client-cloudformation": "^3.948.0",
    "@aws-sdk/client-cloudfront": "^3.985.0",
    "@aws-sdk/client-cloudwatch-logs": "^3.943.0",
    "@aws-sdk/client-dynamodb": "^3.943.0",
    "@aws-sdk/client-ec2": "^3.943.0",
    "@aws-sdk/client-ecr": "^3.943.0",
    "@aws-sdk/client-ecs": "^3.985.0",
    "@aws-sdk/client-efs": "^3.943.0",
    "@aws-sdk/client-elastic-load-balancing-v2": "^3.943.0",
    "@aws-sdk/client-eventbridge": "^3.943.0",
    "@aws-sdk/client-lambda": "^3.943.0",
    "@aws-sdk/client-pricing": "^3.943.0",
    "@aws-sdk/client-s3": "^3.943.0",
    "@aws-sdk/client-ses": "^3.985.0",
    "@aws-sdk/client-sns": "^3.948.0",
    "@aws-sdk/client-ssm": "^3.985.0",
    "@aws-sdk/lib-dynamodb": "^3.992.0",
    "@aws-sdk/s3-request-presigner": "^3.948.0",
    "@aws-sdk/util-dynamodb": "^3.943.0",
    "@smithy/util-waiter": "^4.2.8",
    "aws-cdk-lib": "^2.232.1",
    "cdk-nag": "^2.37.55",
    "chalk": "^5.6.2",
    "commander": "^14.0.3",
    "constructs": "^10.4.4",
    "dotenv": "^16.3.1",
    "source-map-support": "^0.5.21"
  },
  "resolutions": {
    "markdown-it": ">=14.1.1",
    "fast-xml-parser": ">=5.3.6",
    "tar": ">=7.5.8"
  },
  "packageManager": "yarn@4.12.0"
}
