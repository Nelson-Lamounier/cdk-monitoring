    {
      "title": "Cache Performance",
      "type": "row",
      "gridPos": { "h": 1, "w": 24, "x": 0, "y": 60 },
      "collapsed": false
    },
    {
      "title": "Cache Hit Rate (%)",
      "type": "gauge",
      "datasource": { "type": "prometheus", "uid": "prometheus" },
      "gridPos": { "h": 8, "w": 8, "x": 0, "y": 61 },
      "targets": [
        {
          "expr": "sum(rate(nextjs_cache_hits_total{job=\"nextjs-application-metrics\"}[5m])) / (sum(rate(nextjs_cache_hits_total{job=\"nextjs-application-metrics\"}[5m])) + sum(rate(nextjs_cache_misses_total{job=\"nextjs-application-metrics\"}[5m]))) * 100",
          "legendFormat": "Hit Rate",
          "refId": "A"
        }
      ],
      "fieldConfig": {
        "defaults": {
          "unit": "percent",
          "min": 0,
          "max": 100,
          "thresholds": {
            "mode": "absolute",
            "steps": [
              { "color": "red", "value": null },
              { "color": "yellow", "value": 50 },
              { "color": "green", "value": 80 }
            ]
          }
        },
        "overrides": []
      },
      "options": {
        "reduceOptions": { "calcs": ["lastNotNull"] },
        "showThresholdLabels": false,
        "showThresholdMarkers": true
      }
    },
