        },
        {
          "expr": "histogram_quantile(0.95, sum(rate(nextjs_http_response_size_bytes_bucket{job=\"nextjs-application-metrics\"}[5m])) by (le))",
          "legendFormat": "Response p95",
          "refId": "B"
        }
