{
  "annotations": {
    "list": []
  },
  "editable": true,
  "fiscalYearStartMonth": 0,
  "graphTooltip": 2,
  "id": null,
  "links": [],
  "panels": [
    {
      "title": "Application Health",
      "type": "row",
      "gridPos": { "h": 1, "w": 24, "x": 0, "y": 0 },
      "collapsed": false
    },
    {
      "title": "App Status",
      "type": "stat",
      "datasource": { "type": "prometheus", "uid": "prometheus" },
      "gridPos": { "h": 4, "w": 4, "x": 0, "y": 1 },
      "targets": [
        {
          "expr": "nextjs_up{job=\"nextjs-application-metrics\"}",
          "legendFormat": "Status",
          "refId": "A"
        }
      ],
      "fieldConfig": {
        "defaults": {
          "mappings": [
            { "options": { "0": { "text": "DOWN", "color": "red" }, "1": { "text": "UP", "color": "green" } }, "type": "value" }
          ],
          "thresholds": {
            "mode": "absolute",
            "steps": [
              { "color": "red", "value": null },
              { "color": "green", "value": 1 }
            ]
          }
        },
        "overrides": []
      },
      "options": {
        "colorMode": "background",
        "graphMode": "none",
        "textMode": "auto",
        "reduceOptions": { "calcs": ["lastNotNull"] }
      }
    },
    {
      "title": "App Info",
      "type": "table",
      "datasource": { "type": "prometheus", "uid": "prometheus" },
      "gridPos": { "h": 4, "w": 8, "x": 4, "y": 1 },
      "targets": [
        {
          "expr": "nextjs_app_info{job=\"nextjs-application-metrics\"}",
          "legendFormat": "",
          "refId": "A",
          "format": "table",
          "instant": true
        }
      ],
      "transformations": [
        {
          "id": "organize",
          "options": {
            "excludeByName": { "Time": true, "Value": true, "__name__": true, "instance": true, "job": true },
            "renameByName": { "version": "Version", "environment": "Environment", "node_version": "Node.js" }
          }
        }
      ]
    },
    {
      "title": "Scrape Target",
      "type": "stat",
      "datasource": { "type": "prometheus", "uid": "prometheus" },
      "gridPos": { "h": 4, "w": 4, "x": 12, "y": 1 },
      "targets": [
        {
          "expr": "up{job=\"nextjs-application-metrics\"}",
          "legendFormat": "Prometheus Scrape",
          "refId": "A"
        }
      ],
      "fieldConfig": {
        "defaults": {
          "mappings": [
            { "options": { "0": { "text": "DOWN", "color": "red" }, "1": { "text": "UP", "color": "green" } }, "type": "value" }
          ],
          "thresholds": {
            "mode": "absolute",
            "steps": [
              { "color": "red", "value": null },
              { "color": "green", "value": 1 }
            ]
          }
        },
        "overrides": []
      },
      "options": {
        "colorMode": "background",
        "graphMode": "none",
        "textMode": "auto",
        "reduceOptions": { "calcs": ["lastNotNull"] }
      }
    },
    {
      "title": "Uptime",
      "type": "stat",
      "datasource": { "type": "prometheus", "uid": "prometheus" },
      "gridPos": { "h": 4, "w": 4, "x": 16, "y": 1 },
      "targets": [
        {
          "expr": "nextjs_process_start_time_seconds{job=\"nextjs-application-metrics\"}",
          "legendFormat": "",
          "refId": "A"
        }
      ],
      "fieldConfig": {
        "defaults": {
          "unit": "dateTimeFromNow"
        },
        "overrides": []
      },
      "options": {
        "colorMode": "value",
        "graphMode": "none",
        "textMode": "auto",
        "reduceOptions": { "calcs": ["lastNotNull"] }
      }
    },
    {
      "title": "Request Rate",
      "type": "stat",
      "datasource": { "type": "prometheus", "uid": "prometheus" },
      "gridPos": { "h": 4, "w": 4, "x": 20, "y": 1 },
      "targets": [
        {
          "expr": "sum(rate(nextjs_http_request_duration_seconds_count{job=\"nextjs-application-metrics\"}[5m]))",
          "legendFormat": "",
          "refId": "A"
        }
      ],
      "fieldConfig": {
        "defaults": {
          "unit": "reqps",
          "color": { "mode": "thresholds" },
          "thresholds": {
            "mode": "absolute",
            "steps": [
              { "color": "blue", "value": null }
            ]
          }
        },
        "overrides": []
      },
      "options": {
        "colorMode": "value",
        "graphMode": "area",
        "reduceOptions": { "calcs": ["lastNotNull"] }
      }
    },

    {
      "title": "HTTP Performance",
      "type": "row",
      "gridPos": { "h": 1, "w": 24, "x": 0, "y": 5 },
      "collapsed": false
    },
    {
      "title": "Request Rate by Method",
      "type": "timeseries",
      "datasource": { "type": "prometheus", "uid": "prometheus" },
      "gridPos": { "h": 8, "w": 12, "x": 0, "y": 6 },
      "targets": [
        {
          "expr": "sum(rate(nextjs_http_request_duration_seconds_count{job=\"nextjs-application-metrics\"}[5m])) by (method)",
          "legendFormat": "{{ method }}",
          "refId": "A"
        }
      ],
      "fieldConfig": {
        "defaults": {
          "color": { "mode": "palette-classic" },
          "custom": { "fillOpacity": 15, "lineWidth": 2, "pointSize": 5, "stacking": { "mode": "normal" } },
          "unit": "reqps"
        },
        "overrides": []
      }
    },
    {
      "title": "Error Rate (5xx / 4xx %)",
      "type": "timeseries",
      "datasource": { "type": "prometheus", "uid": "prometheus" },
      "gridPos": { "h": 8, "w": 12, "x": 12, "y": 6 },
      "targets": [
        {
          "expr": "sum(rate(nextjs_http_request_duration_seconds_count{job=\"nextjs-application-metrics\", status=~\"5..\"}[5m])) / sum(rate(nextjs_http_request_duration_seconds_count{job=\"nextjs-application-metrics\"}[5m])) * 100",
          "legendFormat": "5xx %",
          "refId": "A"
        },
        {
          "expr": "sum(rate(nextjs_http_request_duration_seconds_count{job=\"nextjs-application-metrics\", status=~\"4..\"}[5m])) / sum(rate(nextjs_http_request_duration_seconds_count{job=\"nextjs-application-metrics\"}[5m])) * 100",
          "legendFormat": "4xx %",
          "refId": "B"
        }
      ],
      "fieldConfig": {
        "defaults": {
          "color": { "mode": "palette-classic" },
          "custom": { "fillOpacity": 20, "lineWidth": 2, "pointSize": 5 },
          "unit": "percent",
          "thresholds": {
            "mode": "absolute",
            "steps": [
              { "color": "green", "value": null },
              { "color": "yellow", "value": 1 },
              { "color": "red", "value": 5 }
            ]
          }
        },
        "overrides": []
      }
    },
    {
      "title": "Request Latency (p50 / p95 / p99)",
      "type": "timeseries",
      "datasource": { "type": "prometheus", "uid": "prometheus" },
      "gridPos": { "h": 8, "w": 12, "x": 0, "y": 14 },
      "targets": [
        {
          "expr": "histogram_quantile(0.50, sum(rate(nextjs_http_request_duration_seconds_bucket{job=\"nextjs-application-metrics\"}[5m])) by (le))",
          "legendFormat": "p50",
          "refId": "A"
        },
        {
          "expr": "histogram_quantile(0.95, sum(rate(nextjs_http_request_duration_seconds_bucket{job=\"nextjs-application-metrics\"}[5m])) by (le))",
          "legendFormat": "p95",
          "refId": "B"
        },
        {
          "expr": "histogram_quantile(0.99, sum(rate(nextjs_http_request_duration_seconds_bucket{job=\"nextjs-application-metrics\"}[5m])) by (le))",
          "legendFormat": "p99",
          "refId": "C"
        }
      ],
      "fieldConfig": {
        "defaults": {
          "color": { "mode": "palette-classic" },
          "custom": { "fillOpacity": 5, "lineWidth": 2, "pointSize": 5 },
          "unit": "s"
        },
        "overrides": []
      }
    },
    {
      "title": "Latency by Route (p95)",
      "type": "timeseries",
      "datasource": { "type": "prometheus", "uid": "prometheus" },
      "gridPos": { "h": 8, "w": 12, "x": 12, "y": 14 },
      "targets": [
        {
          "expr": "histogram_quantile(0.95, sum(rate(nextjs_http_request_duration_seconds_bucket{job=\"nextjs-application-metrics\"}[5m])) by (le, route))",
          "legendFormat": "{{ route }}",
          "refId": "A"
        }
      ],
      "fieldConfig": {
        "defaults": {
          "color": { "mode": "palette-classic" },
          "custom": { "fillOpacity": 5, "lineWidth": 2, "pointSize": 5 },
          "unit": "s"
        },
        "overrides": []
      }
    },
    {
      "title": "Request Size Distribution",
      "type": "timeseries",
      "datasource": { "type": "prometheus", "uid": "prometheus" },
